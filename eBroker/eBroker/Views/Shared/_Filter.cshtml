﻿
<form action="" method="get" class="form-inline pull-right" autocomplete="off" target="_blank">
    <div class="form-group form-group-sm">
        <input type="text" value="@ViewBag.StartDate" required name="StartDate" placeholder="Start date" class="form-control datepicker flat" />
    </div>
    <div class="form-group  form-group-sm">
        <input type="text" value="@ViewBag.EndDate" required name="EndDate" placeholder="End date" class="form-control  datepicker flat" />
    </div>
    <button class="btn btn-primary btn-sm">
        <i class="fa fa-search"></i>
        Go
    </button>
</form>
